{
  "name": "Saka Key",
  "version": "1.21.3",
  "author": "Sufyan Dawoodjee",
  "description": "A keyboard interface to the web",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "background": {
    "scripts": ["background_page.js"]
  },
  "browser_action": {
    "default_icon": {
      "48": "logo.png"
    },
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
    },
  "permissions": [
    "tabs",
    "activeTab",
    "sessions",
    "storage",
    "unlimitedStorage",
    "<all_urls>",
    "clipboardRead",
    "downloads"
  ],
  "web_accessible_resources": [
    "content_script.js",
    "content_script.js.map",
    "background_page.js.map",
    "content_script_loader.js.map",
    "options.js.map",
    "info.js.map",
    "Roboto-Regular.tff",
    "config_basic.json",
    "config_command.json",
    "config_developer.json",
    "config_hints.json",
    "default_basic.json",
    "default_command.json",
    "default_developer.json",
    "default_hints.json"
    ]
}
